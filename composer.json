{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name": "drmonty/smartmenus",
                "version": "1.1.1",
                "type": "drupal-library",
                "dist": {
                    "url": "https://www.smartmenus.org/files/?file=smartmenus-jquery/smartmenus-1.1.1.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "^2.0"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2.0",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.4",
        "drupal/all_entity_preview": "^1.0@alpha",
        "drupal/bg_image_formatter": "^1.17",
        "drupal/block_field": "^1.0@RC",
        "drupal/content_templates": "^1.1",
        "drupal/core-composer-scaffold": "10.3.0-beta1",
        "drupal/core-dev": "^10.2",
        "drupal/core-project-message": "10.3.0-beta1",
        "drupal/core-recommended": "^10.3",
        "drupal/diff": "^1.3",
        "drupal/easy_breadcrumb": "^2.0",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/field_group": "^3.4",
        "drupal/focal_point": "^2.1",
        "drupal/fontawesome": "^2.26",
        "drupal/frontend_editing": "^1.7",
        "drupal/gin": "^3.0@RC",
        "drupal/gin_login": "^2.0",
        "drupal/imagemagick": "^4.0@beta",
        "drupal/key": "^1.17",
        "drupal/layout_paragraphs": "^2.0",
        "drupal/libraries": "^4.0",
        "drupal/masquerade": "^2.0@RC",
        "drupal/material_icons": "^2.0",
        "drupal/node_edit_protection": "^1.1",
        "drupal/paragraphs": "^1.17",
        "drupal/paragraphs_responsive_background_image_formatter": "^1.0",
        "drupal/pathauto": "^1.12",
        "drupal/require_revision_log_message": "^2.0",
        "drupal/responsive_tables_filter": "^1.18",
        "drupal/sdc_display": "^1.0@beta",
        "drupal/smartmenus": "^2.0@beta",
        "drupal/storybook": "^1.0@beta",
        "drupal/svg_image": "^3.0",
        "drupal/tb_megamenu": "^3.0@alpha",
        "drupal/tocbot": "^1.0@alpha",
        "drupal/token": "^1.14",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^3.3",
        "drupal/webp": "^1.0@RC",
        "drush/drush": "^12"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "phpstan/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "cweagans/composer-patches": true
        },
        "sort-packages": true
    },
    "extra": {
        "enable-patching": true,
        "patches-file": "composer.patches.json",
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/installing-drupal",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
