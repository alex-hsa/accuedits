<figure class="h-full">
{% if storybook %}
  <img class="bg-auto{{classes}}" src="{{ image }}" alt="{{ image_alt }}" />
{% else %}
  {# {{ drupal_image(image, image_style, { alt: image_alt, title: image_alt } ) }} #}
  {{ drupal_image(image, image_style, { alt: image_alt, title: image_alt, class: classes }, responsive=true ) }}
{% endif %}
</figure>